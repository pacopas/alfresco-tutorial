<alfresco-config>

	<config evaluator="node-type" condition="tut:hcContent">
		<property-sheet>
			<show-property name="tut:specialty" display-label-id="specialty" />
		</property-sheet>
	</config>

	<!-- add tutorial types to add content list -->
	<config evaluator="string-compare" condition="Content Wizards">
		<content-types>
			<type name="tut:hcContent" />
		</content-types>
	</config>

</alfresco-config>